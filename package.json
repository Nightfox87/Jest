{
  "name": "jest",
  "version": "1.0.0",
  "description": "jest test",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverage"
  },
  "keywords": [
    "jest",
    "test",
    "jsaqa"
  ],
  "author": "abramova",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "collectCoverage": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  }
}
